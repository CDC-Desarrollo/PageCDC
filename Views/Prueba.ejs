
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <style>
        .Galeria1{
            width: 300px;
            height: 200px;
        }
    </style>
</head>
<body>
    <div id="galeria"> </div>

    <button id="btnCargarImagen">Cargar Imagen</button>
    <script>

        document.getElementById('btnCargarImagen').addEventListener('click',async() => {
            let response = await fetch("http://localhost:8080/api/Images", { 
            method: "GET",
            });

            let data = await response.json();
            console.log(data);

            const galeria=document.getElementById('galeria')
            console.log(galeria);

            let num=1;
            data.forEach(element => {
             
                const imagenes = document.createElement('img');
                // imagenes.id="img"+num;
                // num++;
                
                imagenes.className='Galeria1'

                imagenes.src=element;
                
                galeria.appendChild(imagenes)
            });
        
        });


    </script>
</body>
</html>